{
  "name": "eleventy-tailwind-template",
  "version": "1.0.0",
  "author": "Jeremy Daly",
  "description": "website",
  "scripts": {
    "start": "npm-run-all -p dev:*",
    "build": "npx @11ty/eleventy && tailwindcss -i _site/css/styles.sass -o dist/css/styles.css",
    "dev:11ty": "eleventy --serve --quiet",
    "dev:css": "tailwindcss -i _site/css/styles.sass -o dist/css/styles.css --watch --postcss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremydaly/eleventy-tailwind-template.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeremydaly/eleventy-tailwind-template/issues"
  },
  "homepage": "https://github.com/jeremydaly/eleventy-tailwind-template#readme",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@tailwindcss/typography": "^0.5.9",
    "alpinejs": "^3.12.0",
    "autoprefixer": "^10.4.0",
    "dayjs": "^1.10.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.0.12"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^3.0.0",
    "eleventy-plugin-activity-pub": "^0.1.1",
    "eleventy-plugin-footnotes": "^0.11.0",
    "markdown-it-footnote": "^3.0.3"
  }
}
